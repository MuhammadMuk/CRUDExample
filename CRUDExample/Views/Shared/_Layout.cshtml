<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/Stylesheet.css" rel="stylesheet" />
    <link rel="stylesheet" 
        href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.2/css/all.min.css" 
        integrity="sha512-SnH5WK+bZxgPHs44uWIX+LLJAJ9/2PkPKZ5QiAj6Ta86w+fsb2TkcmfRyVX3pBnMFcV7oQPJkl9QevSCWr3W6A==" 
        crossorigin="anonymous" referrerpolicy="no-referrer" />
</head>
<body>
    <div>
        <div class="container">
            <div class="navbar">
                <div class="navebar-brand">
                    <img src="~/logo.png" width="45px" height="45px" asp-append-version="true"/>
                </div>
                <ul>
                    <li>
                        <a asp-action="Index" asp-controller="Persons">Persons</a>
                        <a asp-action="UploadFromExcel" asp-controller="Countries">Upload Countries Excel</a>

                    </li>
                </ul>
            </div>
            <div class="page-content">
                @RenderBody()
            </div>
        </div>
    </div>
    @RenderSection("scripts",required: false)
</body>
</html>
